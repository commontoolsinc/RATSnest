{
  "tasks": {
    "dev:api": "deno run --allow-net --allow-read --watch main.ts",
    "dev:tunnel": "deno run --allow-net --allow-read --allow-env --watch tunnel.ts",
    "dev": "deno run --allow-net --allow-read --allow-env --watch tunnel.ts",
    "build": "cd ../frontend && npm run build && cd ../backend && mkdir -p dist && deno compile --allow-all --include=../frontend/dist --output=dist/ratsnest main.ts"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "hono": "jsr:@hono/hono@^4",
    "express": "npm:express@^4",
    "@teekit/tunnel": "npm:@teekit/tunnel@0.0.1",
    "@types/express": "npm:@types/express@^4"
  }
}
